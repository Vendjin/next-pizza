{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "jsx-a11y/alt-text": "off",
    "prettier/prettier": [
      "warn"
    ],
    "max-len": [
      "off",
      {
        "code": 120
      }
    ]
  }
}
